generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = "mysql://root:root@localhost:3306/Xuchil"
}

model product {
  product_id     Int     @id @default(autoincrement())
  category       String  @db.VarChar(50)
  name           String  @db.VarChar(100)
  is_end_product Boolean
}
